<?xml version="1.0" encoding="UTF-8"?>
<plugins>

	<path name="TooltipAdderResources">
		<segment root="resources"/>
		<segment append="plugins"/>
		<segment append="TooltipAdder"/>
	</path>

	<plugin name="generationBloodpoolSynchronizer">
		<jar class="vampire.editor.plugin.generationbloodpoolsynchronizer.application.Constructor"
			path="plugins" file = "GenerationBloodpoolSynchronizer/GenerationBloodpoolSynchronizer.jar"/>
	</plugin>
	
	<plugin name="dynamicSubCategoryView">
		<jar class="vampire.editor.plugin.dynamicsubcategorymanager.application.Constructor"
			path="plugins" file ="DynamicSubCategoryTraitManager/DynamicSubCategoryTraitManager.jar"/>
	</plugin>
	
	<plugin name="dynamicMeritsView">
		<jar class="vampire.editor.plugin.dynamicmeritentrymanager.application.Constructor"
			path="plugins" file ="DynamicMeritEntryManager/DynamicMeritEntryManager.jar"/>
	</plugin>
	
	<dictionary key="tooltips" path="TooltipAdderResources" file="tooltips"/>
	
	<plugin name="TooltipAdder">
		<jar class="vampire.editor.plugin.tooltipadder.Constructor"
			path="plugins" file="TooltipAdder/TooltipAdder.jar"/>
	</plugin>
	
	<plugin name="TraitPopupEntryAdder">
		<jar class="vampire.editor.plugin.popupentryadder.Constructor"
			path="plugins" file="TraitPopupEntryAdder/TraitPopupEntryAdder.jar"/>
	</plugin>	
	
	<plugin name="MetaPopupEntryAdder">
		<jar class="vampire.editor.plugin.metapopupentryadder.Constructor"
			path="plugins" file="MetaPopupEntryAdder/MetaPopupEntryAdder.jar"/>
	</plugin>
	
	<plugin name="Sheetsummary">
		<jar class="vampire.editor.plugin.sheetsummary.application.Constructor"
			path="plugins" file="SheetSummary/SheetSummary.jar"/>
	</plugin>
	
	<!-- plugin name="freebieCalculator">
		<jar class="vampire.editor.plugin.freebiecalculator.application.Activator"/>
	</plugin -->
	
</plugins>